# -*- coding: utf-8 -*-
# vim:fenc=utf-8 ff=unix

# git system files
.gitattributes eol=lf
.gitmodules    eol=lf
# gitignore contains Icon^M <- CRLF end
# skip autocrlf,diff enable -> not binary, set -text
.gitignore     -text

.alexignore    eol=lf

# Extension
*.sh           text eol=lf
*.py           text eol=lf

# File
Dockerfile     text eol=lf

# EOF
